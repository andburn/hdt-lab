<UserControl x:Class="HDT.Plugins.EndGame.Controls.PluginSettings"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:HDT.Plugins.EndGame.Controls"
             mc:Ignorable="d" d:DesignWidth="300" d:DesignHeight="500">
    <TabControl TabStripPlacement="Top">
        <TabItem x:Name="ArchetypeSettings" Header="Archetype">
            <local:ArchetypeSettings />
        </TabItem>
        <TabItem x:Name="ScreenshotSettings" Header="Screenshot">
            <local:ScreenshotSettings />
        </TabItem>
    </TabControl>
</UserControl>